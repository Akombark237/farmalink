{
  "name": "phamarlink",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node start-with-chatbot.js",
    "dev:frontend-only": "next dev",
    "dev:full": "node start-with-chatbot.js",
    "dev:chatbot": "powershell -ExecutionPolicy Bypass -File start-with-chatbot.ps1",
    "build": "next build",
    "start": "next start",
    "start:full": "node start-with-chatbot.js",
    "lint": "next lint",
    "dev-fast": "NEXT_TELEMETRY_DISABLED=1 node --max-old-space-size=512 node_modules/next/dist/bin/next dev",
    "dev-ultra": "NEXT_CONFIG_FILE=next.config.minimal.js NEXT_TELEMETRY_DISABLED=1 node --max-old-space-size=256 node_modules/next/dist/bin/next dev",
    "optimize": "node optimize-project.js",
    "simple": "node simple-server.js",
    "chatbot:start": "cd gemini-proxy && npm start",
    "chatbot:dev": "cd gemini-proxy && npm run dev",
    "chatbot:test": "curl http://localhost:3001/health || echo 'Chatbot not responding'",
    "db:migrate": "node scripts/migrate.js migrate",
    "db:rollback": "node scripts/migrate.js rollback",
    "db:seed": "psql -h localhost -U postgres -d pharmacy_platform -f database/06_sample_data.sql",
    "db:reset": "npm run db:rollback && npm run db:migrate && npm run db:seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:all": "npm run test && npm run test:e2e"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@huggingface/inference": "^4.0.5",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@types/bcryptjs": "^2.4.6",
    "@types/socket.io": "^3.0.1",
    "@types/socket.io-client": "^1.4.36",
    "@xenova/transformers": "^2.17.2",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.12.1",
    "ioredis": "^5.6.1",
    "isomorphic-dompurify": "^2.25.0",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "leaflet-routing-machine": "^3.2.12",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "next-pwa": "^5.6.0",
    "pg": "^8.16.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "recharts": "^2.15.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.18",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "supertest": "^7.1.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
