{
  "name": "phamarlink",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:full": "powershell -ExecutionPolicy Bypass -File start-full-project.ps1",
    "dev:chatbot": "cd phamarlink/gemini-proxy && set PORT=3003 && npm run dev",
    "start:chatbot": "cd phamarlink/gemini-proxy && set PORT=3003 && npm start",
    "check": "node check-services.js",
    "check:ps": "powershell -ExecutionPolicy Bypass -File check-services.ps1",
    "build": "echo 'Building Next.js application...' && next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "node scripts/migrate.js migrate",
    "db:rollback": "node scripts/migrate.js rollback",
    "db:seed": "psql -h localhost -U postgres -d pharmacy_platform -f database/06_sample_data.sql",
    "db:reset": "npm run db:rollback && npm run db:migrate && npm run db:seed",
    "deploy": "bash deploy.sh",
    "deploy:windows": "powershell -ExecutionPolicy Bypass -File deploy.ps1",
    "deploy:pm2": "pm2 start ecosystem.config.js --env production",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=.next",
    "deploy:render": "echo 'Deploy to Render via GitHub integration'",
    "build:render": "bash build.sh",
    "test:api": "node scripts/test-all-apis.js",
    "test:api:ps": "powershell -ExecutionPolicy Bypass -File scripts/test-apis.ps1"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@react-google-maps/api": "^2.20.6",
    "@types/pg": "^8.15.4",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.12.1",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "pg": "^8.16.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "recharts": "^2.15.3",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/google.maps": "^3.58.1",
    "@types/leaflet": "^1.9.18",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
